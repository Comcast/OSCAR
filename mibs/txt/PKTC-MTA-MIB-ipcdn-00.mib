            MODULE-COMPLIANCE,   
            NOTIFICATION-GROUP   
                     FROM SNMPv2-CONF   
            InetAddressType, InetAddress 
                     FROM INET-ADDRESS-MIB  
            sysDescr 
                     FROM SNMPv2-MIB 
            SnmpAdminString 
                     FROM SNMP-FRAMEWORK-MIB 
            docsDevSwCurrentVers   
                     FROM DOCS-CABLE-DEVICE-MIB; -- version 8  
