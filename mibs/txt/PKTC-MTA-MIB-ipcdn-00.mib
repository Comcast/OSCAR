   PKTC-MTA-MIB DEFINITIONS ::= BEGIN 
    
       IMPORTS   
            MODULE-IDENTITY,   
            OBJECT-TYPE,   
            Integer32,   
            Counter32,   
            NOTIFICATION-TYPE,   
            mib-2 
                     FROM SNMPv2-SMI 
            TruthValue,   
            RowStatus, 
            TEXTUAL-CONVENTION   
                     FROM SNMPv2-TC   
